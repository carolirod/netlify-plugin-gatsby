    "download": "^8.0.0",
    "etag": "^1.8.1",
    "fs-extra": "^10.0.0",
    "linkfs": "^2.1.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "path-to-regexp": "^0.1.7",
    "tempy": "^1.0.0"
  },
  "devDependencies": {
    "@netlify/build": "^18.21.2",
    "@types/etag": "^1.8.1",
    "@types/fs-extra": "^9.0.12",
    "@types/multer": "^1.4.7",
    "gatsby": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "release-it": "^14.8.0",
    "rimraf": "^3.0.2"
  }
